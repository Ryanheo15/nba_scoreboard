
<%- include('header'); -%>
    <% games.forEach(game=>{%> 
    <!-- Create whatever game blocks using data from games -->
    <div class="table">

      <div class="row">
        <div class="col-lg-1">
          <h1> <%=game.HomeScore%> </h1>
          <img src="" alt="">
        </div>
        <div class="col-lg-8">
          <h1><%=game.status%></h1>
        </div>
        <div class="col-lg-1">
          <h1><%=game.AwayScore%></h1>
        </div>

      </div>

      <div class="row">
        <div class="col-lg-1">
          <img src=<%=game.HomeLogo%> alt="" width = "100" height = "100">
        </div>
        <div class="col-lg-8" style = " text-align: center;">
          <h2> <%=game.arena%></h2>
        </div>
        <div class="col-lg-1">
          <img src=<%=game.AwayLogo%> alt="" width = "100" height = "100">
        </div>

      </div>

      <div class="row">
        <div class="col-lg-1">
          <p><%=game.Home%> </p>
        </div>
        <div class="col-lg-8">
          <a href= <%=game.dateGameId%>>Boxscore</a>
        </div>
        <div class="col-lg-1">
          <p><%=game.Away%></p>
        </div>

      </div>

    </div>
    <%})%>


<%- include('footer'); -%>